{
  "name": "@illgrenoble/webx-client",
  "description": "Client javascript library to display WebX remote desktop in a browser.",
  "version": "1.9.3",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "copy": "cp src/input/GuacamoleKeyboard.js dist/input/GuacamoleKeyboard.js",
    "build": "tsc && npm run copy",
    "build:watch": "nodemon --watch src --ext ts,js --exec 'npm run build'"
  },
  "keywords": [
    "webx"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/ILLGrenoble/webx-client.git"
  },
  "license": "GPL-3.0",
  "author": "Stuart Caunt <caunt@ill.fr>",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/three": "^0.174.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "buffer": "^6.0.3",
    "eslint": "^7.32.0",
    "nodemon": "^3.1.10",
    "prettier": "^2.4.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^25.0.0",
    "three": "^0.174.0"
  }
}
